{
	"schemaVersion": 1,
	"id": "charmncrafts-gears-and-gadgets",
	"version": "${version}",
	"name": "CharmNCrafts: Gears and Gadgets",
	"description": "A modular Minecraft mod featuring Arsenal & Armory, Gears & Gadgets, and more! Adds powerful tools, weapons, and utility items to enhance your gameplay.",
	"authors": [
		"CharmNCrafts Team"
	],
	"contact": {
		"homepage": "https://github.com/LiterallyLink/CharmNCrafts-Arsenal-And-Armory",
		"sources": "https://github.com/LiterallyLink/CharmNCrafts-Arsenal-And-Armory"
	},
	"license": "CC0-1.0",
	"icon": "assets/charmncrafts-gears-and-gadgets/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"chamncraft.core.CharmNCraftsMod"
		],
		"client": [
			"chamncraft.core.CharmNCraftsModClient"
		],
		"fabric-datagen": [
			"chamncraft.core.CharmNCraftsModDataGenerator"
		]
	},
	"mixins": [
		"charmncrafts-gears-and-gadgets.mixins.json",
		{
			"config": "charmncrafts-gears-and-gadgets.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}